GL <- GL %>%#
  mutate(DE = ifelse(GL$DE == "", "U", as.character(DE))) %>%#
  mutate(LMAp_up =  GL_summary[P_vec, "X97.5."] * LMA) %>%#
  mutate(LMAs_lo = LMA - LMAp_up) %>%#
