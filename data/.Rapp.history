countR <- function(gx, gy, r){#
 n_length <- length(gx)#
 res <- numeric(n_length)#
 for (i in 1:n_length){#
  temp <- numeric(n_length)#
   for (j in 1:n_length){#
        if ((gx[i]-gx[j])^2 + (gy[i]-gy[j])^2 <= r^2) temp[j] <- 1#
      }#
      res[i] <- sum(temp)#
      temp <- numeric(n_length)#
    }#
  res - 1#
}
